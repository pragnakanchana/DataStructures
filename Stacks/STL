#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    stack<int> s;
    s.push(10);
    s.push(20);
    s.push(30);
    cout << s.top() << endl;
    s.pop();
    cout << s.top() << endl;
    cout << "size : " << s.size() << endl;
    cout << "IsEmpty : " << s.empty() << endl;
    while (!s.empty()) {
        cout << ' ' << s.top();
        s.pop();
    }
    return 0;
}
