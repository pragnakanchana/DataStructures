#include <iostream>
#include<bits/stdc++.h>
using namespace std;


int main()
{
    stack<int> s;
    s.push(10);
    s.push(20);
    cout << s.top() << endl;
    s.pop();
    cout << s.top() << endl;
    cout << "size : " << s.size() << endl;
    cout << "IsEmpty : " << s.empty() << endl;
    return 0;
}
